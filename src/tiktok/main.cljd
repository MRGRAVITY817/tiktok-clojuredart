(ns tiktok.main
  (:require ["package:flutter/material.dart" :as m]
            [cljd.flutter :as f]
            [tiktok.features.authentication.sign-up-screen :refer [sign-up-screen]]))

(defn main []
  (f/run
   (m/MaterialApp
    .title "TikTok"
    .theme (m/ThemeData
            .primaryColor (m/Color 0xFFE9435A))
    .home (sign-up-screen))))


